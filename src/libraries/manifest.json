{
    "mode": "dev",
    "out": {
        "name": "cat",
        "folder": "lib"
    },
    "libraries": [
        {
            "name": "cat",
            "address": "local",
            "base": "target",
            "prod" :[
                {
                    "name": "cat.js",
                    "src": ["Cat.js", "core/**/*.js", "plugins/**/*.js", "CatSettings.js"],
                    "type": "js"
                },
                {
                    "name": "cat.css",
                    "src": ["css/*.css"],
                    "type": "css"
                },
                {
                    "name": "cat.json",
                    "src": ["config/catconfig.json"],
                    "type": "json"
                }
            ],
            "dev" :[
                {
                    "name":"cat.js",
                    "src": ["Cat.js", "core/**/*.js", "plugins/**/*.js", "CatSettings.js"],
                    "type": "js"
                },
                {
                    "name": "cat.css",
                    "src": ["css/*.css"],
                    "type": "css"
                },
                {
                    "name": "cat.json",
                    "src": ["config/catconfig.json"],
                    "type": "json",
                    "parse": true
                }
            ],
            "static-names": ["catconfig.json"],
            "install" : "internal"
        },
        {
            "name": "requirejs",
            "address": "local",
            "base": ".",
            "prod" :["requirejs.js"],
            "dev" :["requirejs.js"],
            "install" : "static"
        },
        {
            "name": "underscore",
            "address": "local",
            "base": ".",
            "prod" :["underscore-min.js", "underscore-min.map"],
            "dev" :["underscore-min.js", "underscore-min.map"],
            "exports": "_",
            "install" : "static"
        },
        {
            "name": "js.utils",
            "address": "local",
            "base": ".",
            "prod" :["target/jsutils-min.js"],
            "dev" :["target/jsutils-min.js"],
            "install" : "static",
            "deps": ["underscore"]
        },
        {
            "name": "tmr",
            "address": "local",
            "base": ".",
            "prod" :["target/tmr-min.js"],
            "dev" :["target/tmr-min.js"],
            "install" : "static",
            "deps": ["underscore", "jsutils"]
        },
        {
            "name": "chai",
            "prod" :["chai.js"],
            "dev" :["chai.js"],
            "exports": "chai",
            "globals": true,
            "install": "static"
        },
        {
            "name": "flyer",
            "prod" :["flyer.min.js"],
            "dev" :["flyer.min.js"],
            "exports": "flyer",
            "globals": true,
            "install": "static"
        },
        {
            "name": "jspath",
            "prod" :["lib/jspath.js"],
            "dev" :["lib/jspath.js"],
            "exports": "JSPath",
            "globals": true,
            "install": "static"
        },
        {
            "name": "jasmine",
            "prod" :["lib/jasmine.js", "lib/jasmine-tap-reporter.js", "lib/catjs-jasmine-boot.js", "lib/jasmine-console.js", "lib/jasmine-boot.js", "lib/jasmine-html.js", "lib/jasmine.css", "lib/jasmine_favicon.png", "lib/jasmine-spec-runner.html"],
            "dev" :["lib/jasmine.js", "lib/jasmine-tap-reporter.js", "lib/catjs-jasmine-boot.js", "lib/jasmine-console.js", "lib/jasmine-boot.js", "lib/jasmine-html.js", "lib/jasmine.css", "lib/jasmine_favicon.png", "lib/jasmine-spec-runner.html"],            
            "exports": "jasmine",
            "globals": true,
            "install": "static"
        },
        {
            "name": "html2canvas",
            "prod" :["html2canvas.min.js", "html2canvas.svg.min.js"],
            "dev" :["html2canvas.min.js", "html2canvas.svg.min.js"],        
            "exports": "html2canvas",
            "globals": true,
            "install": "static"
        },
        {
            "name": "q",
            "prod" :["q.min.js"],
            "dev" :["q.min.js"],        
            "exports": "q",
            "globals": true,
            "install": "static"
        },
        {
            "name": "canvg",
            "prod" :["canvg.min.js"],
            "dev" :["canvg.min.js"],        
            "exports": "canvg",
            "globals": true,
            "install": "static"
        }
    ]
}