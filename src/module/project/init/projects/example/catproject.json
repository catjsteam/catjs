{
    "name": "{{name}}",

    "source": "src/",
    "target": "target/",
    "cattarget": "{{cattarget}}",

    "analytics": "{{analytics}}",
    "apppath": "{{appath}}",

    "server": {
        "host": "{{server.host}}",
        "port": "{{server.port}}",
        "protocol": "{{server.protocol}}",
        "services": {
            "static-pages": true
        }
    },
    
    "runner": {
        "run": {
            "devices": [
                {
                    "type": "localpc",
                    "runner": {
                        "name": "chrome",
                        "address": "/index.html"
                    }
                },
                {
                    "type": "localpc",
                    "disable": true,
                    "runner": {
                        "name": "firefox",
                        "address": "/index.html"
                    }
                },
                {
                    "type": "android",
                    "disable": true,
                    "id": "all",
                    "runner": {
                        "name": "apk"
                    }
                }

            ]
        }
    },

    "plugins": [

    ],

    "tasks": [

    ]


}
